{
  "kind": "collectionType",
  "collectionName": "boutiques",
  "info": {
    "singularName": "boutique",
    "pluralName": "boutiques",
    "displayName": "Boutique",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "nom": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "categorie": {
      "type": "string",
      "required": true
    },
    "emplacement": {
      "type": "string",
      "required": true
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "banniere": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "boutiques"
    },
    "situation": {
      "type": "string",
      "required": true,
      "default": "En attente"
    }
  }
}
